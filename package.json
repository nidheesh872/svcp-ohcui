{
  "name": "svcp-ohcui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@date-io/moment": "^1.3.13",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^5.0.0-beta.4",
    "@testing-library/react-hooks": "^3.7.0",
    "@types/crypto-js": "^4.0.2",
    "@types/lodash": "^4.14.165",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/segment-analytics": "0.0.33",
    "crypto-js": "^4.1.1",
    "formik": "^2.2.5",
    "formik-material-ui": "^3.0.0-alpha.0",
    "formik-material-ui-lab": "0.0.7",
    "formik-material-ui-pickers": "0.0.11",
    "i18next": "^19.7.0",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-http-backend": "^1.0.21",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "react": "^17.0.1",
    "react-csv": "^2.0.3",
    "react-dom": "^17.0.1",
    "react-draggable": "^4.4.3",
    "react-file-reader": "^1.1.4",
    "react-i18next": "^11.7.3",
    "react-loading-skeleton": "^2.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5.0.0",
    "react-use-ref-effect": "^1.1.0",
    "universal-cookie": "^4.0.4",
    "useref": "^1.4.3",
    "web-vitals": "^1.0.1",
    "wretch": "^1.7.4",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:dev": "env-cmd -f .env.dev react-scripts build",
    "build:qa": "env-cmd -f .env.qa react-scripts build",
    "build:prod": "env-cmd -f .env.prod react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "test2": "react-scripts test --env=jest-environment-jsdom-sixteen --silent",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache --watchAll=false --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install",
    "create-hook": "husky add .husky/pre-commit \"npm test\"",
    "pre-commit": "lint-staged"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ],
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/no-unused-vars": "off"
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^10.4.9",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^27.4.1",
    "@types/react-csv": "^1.1.2",
    "@types/yup": "^0.29.6",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "html-loader": "^2.1.2",
    "http-proxy-middleware": "^1.0.4",
    "husky": "^6.0.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "lint-staged": "^10.2.11",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.2",
    "react-is": "^16.13.1",
    "react-test-renderer": "^16.14.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        2,
        "never",
        [
          "start-case",
          "pascal-case"
        ]
      ],
      "subject-min-length": [
        2,
        "always",
        12
      ]
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "CI=true npm test"
    }
  },
  "lint-staged": {
    "src/*/.{js,jsx,ts,tsx,json,css,scss,md}": [
      "eslint --cache --fix"
    ]
  }
}
